{
  "name": "vite-plugin-vue-atomcss",
  "version": "0.0.9",
  "description": "vite-plugin-vue-atomcss",
  "main": "atomcss.js",
  "type": "module",
  "exports": {
    ".": {
      "require": "./atomcss.cjs",
      "import": "./atomcss.js"
    },
    "./atomcss.css": {
      "require": "./atomcss.css",
      "import": "./atomcss.css"
    }
  },
  "files": [
    "atomcss.cjs",
    "atomcss.js",
    "atomcss.config.js",
    "atomcss-common.css",
    "atomcss-generated.css",
    "atomcss.css",
    "client.js"
  ],
  "scripts": {
    "dev": "cd package && npm run dev",
    "copy": "cp package/plugin/*.css package/plugin/atomcss.config.js package/plugin/client.js . && echo '' > atomcss-generated.css",
    "build": "npm run copy && tsup package/plugin/atomcss.js --minify --format esm,cjs -d ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wujr5/vite-plugin-vue-atomcss.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "vue",
    "atomcss"
  ],
  "author": "吴家荣",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wujr5/vite-plugin-vue-atomcss/issues"
  },
  "homepage": "https://github.com/wujr5/vite-plugin-vue-atomcss#readme",
  "devDependencies": {
    "tsup": "6.6.3"
  }
}
